{
  "name": "starter-cra-and-react",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "concurrently": "^5.0.2",
    "cross-env": "^10.1.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^3.4.0"
  },
  "scripts": {
    "start": "npm run development",
    "development": "cross-env NODE_ENV=development concurrently --kill-others \"npm run client\" \"npm run server\"",
    "production": "npm run build && cross-env NODE_ENV=production npm run server",
    "server": "node server/server.js",
    "client": "cross-env NODE_OPTIONS=--openssl-legacy-provider react-scripts start",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "engines": {
    "node": "10.x"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "license": "MIT"
}